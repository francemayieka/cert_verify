{
  "version": 2,
  "name": "certverify",
  "builds": [
    {
      "src": "CertVerifyProject/CertVerify/manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "CertVerifyProject/CertVerify/CertVerify/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "CertVerifyProject/CertVerify/CertVerify/wsgi.py"
    }
  ]
}
