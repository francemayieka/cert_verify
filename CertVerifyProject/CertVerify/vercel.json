{
  "version": 2,
  "name": "certverify",
  "builds": [
    {
      "src": "CertVerifyProject/CertVerify/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "CertVerifyProject/CertVerify/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "CertVerify.settings",
    "DJANGO_SECRET_KEY": "django-insecure-a-re@_b^pr_^q^^67(9y7dd0&ih$g$u8005+&5=9j8jscx##gr",
    "DATABASE_URL": "sqlite:///db.sqlite3",
    "DEBUG": "false",
    "ALLOWED_HOSTS": "*"
  },
  "buildCommand": "python manage.py collectstatic --noinput && python manage.py migrate"
}
